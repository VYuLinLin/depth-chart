{
  "name": "@real2sport/depth-chart",
  "version": "1.0.1",
  "description": "一个使用vue3和vue-konva的深度图组件",
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "main": "dist/depth-chart.cjs",
  "module": "dist/depth-chart.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "bignumber.js": "^9.1.2",
    "vue": "^3.5.13",
    "vue-konva": "^3.2.0"
